# -eq, -ne, -gt, -ge, -lt, -le gibi ifadeler ve stringler için [ ] kullanılır.
# >, <, = ifadeleri için ( ) kullanılır.

# string karşılaştırmada alfabetik dizilime göre karşılaştırma yapmak için [[ ]] kullanılır.

read -p "Input 2 integers = " num1 num2
echo "Your integers are $num1 and $num2."

if [ $num1 -eq $num2 ]
then
  echo "$num1 and $num2 are equal."
else
  echo "$num1 and $num2 are not equal."
fi
